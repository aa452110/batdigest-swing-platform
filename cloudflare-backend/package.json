{
  "name": "swing-analysis-api",
  "version": "1.0.0",
  "description": "Cloudflare Worker API for Baseball Swing Analysis",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create swing-analysis",
    "db:init": "wrangler d1 execute swing-analysis --local --file=./schema.sql",
    "db:migrate": "wrangler d1 execute swing-analysis --file=./schema.sql",
    "tail": "wrangler tail"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240806.0",
    "typescript": "^5.5.4",
    "wrangler": "^3.67.1"
  },
  "dependencies": {}
}